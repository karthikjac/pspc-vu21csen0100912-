#include <stdio.h>
int main()
{
   int a,b,choice;
   printf("enter your choice\n");
   printf(" 1.addition\n, 2.subtraction\n 3. multiplication\n 4.division\n");
   scanf("%d",&choice);
   switch(choice)
   {
       case 1: 
       printf("%d+%d=%d\n",a,b, a+b);
       break;
       case 2:
       printf("%d-%d=%d\n",a,b, a-b);
       break;
       case 3:
       printf("%d*%d=%d\n",a,b, a*b);
       break;
       case 4:
       printf("%d/%d=%d\n",a,b, a/b);
       break;
   }

   return 0;
}
