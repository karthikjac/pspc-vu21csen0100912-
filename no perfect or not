#include <stdio.h>
int main()
{
	int a,b;
	int sum;
	printf("enter an integer number");
	scanf("%d",&a);
	  sum = 0;

    for (b = 1; b < a; b++) {
        if (a % b == 0)
            sum += b;
    }

    if (sum == a)
        printf("%d is a perfect number.", a);
    else
        printf("%d is not a perfect number.", a);

    return 0;
}
