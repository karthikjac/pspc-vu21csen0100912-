#include <stdio.h>
int main()
{
    int week;
    printf("Enter week number(1-7): ");
    scanf("%d", &week);
    
    switch(week)
    {
        case 1: 
            printf("Monday");
            break;
        case 2: 
            printf("Tuesday");
            break;
        case 3: 
            printf("Wednesday");
            break;
        case 4: 
            printf("Thursday");
            break;
        case 5: 
            printf("friday");
            break;
        case 6:
            printf("saturday");
        case 7:
            printf("sunday");
        default: 
            printf("Invalid input! Please enter week number between 1-7.");
    }
    return 0;
}
