#include<stdio.h>
int main()
{
int a,b,c,e;
printf("enter the first side a");
scanf("%d",&a);
printf("enter the secon side b");
scanf("%d",&b);
printf("enter the third side c");
scanf("%d",&c);
if(a==b&&b==c)
printf("the given triangle is equilateral");
else if(a==b||b==c||c==a)
printf("the given triangle is isosceles");
else
printf("the given triangle is scalene");
return 0;
}


   
