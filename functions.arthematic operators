#include <stdio.h>
void add();
void sub();
void mul();
void div();
void main(){

    int a,b,c;
    printf("function calling my self");
    add();
    sub();
    mul();
    div();
}
  void add ()
{
    int a,b,c;
    printf("enter the value of a and b");
    scanf("%d%d",&a,&b);
    c=a+b;
    printf("%d",c);
}
     void sub ()
{
    int a,b,c;
    printf("enter the value of a and b");
    scanf("%d%d",&a,&b);
    c=a-b;
    printf("%d",c);
}
 void mul ()
{
    int a,b,c;
    printf("enter the value of a and b");
    scanf("%d%d",&a,&b);
    c=a*b;
    printf("%d",c);
}
 void div ()
{
    int a,b,c;
    printf("enter the value of a and b");
    scanf("%d%d",&a,&b);
    c=a/b;
    printf("%d",c);
}
